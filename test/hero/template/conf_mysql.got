<%: func FromConfMysql(dbName string, buffer *bytes.Buffer) %>
[db<%= strings.Title(dbName) %>]
dbHost = ""           #数据库连接地址
dbName = "<%= dbName %>` + dbName + `"           #数据库名称
dbUser = ""           #数据库用户名
dbPasswd = ""         #数据库密码
dbPort = "3306"       #数据库端口号
dbOpenconns_max = 20  #最大连接数
dbIdleconns_max = 20  #最大空闲连接
dbType = "mysql"